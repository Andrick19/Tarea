package cuenta;
public class CuentaAhorro extends CuentaBancaria{
    
    
    public double deposito(double deposito, double monto){
        double aumento;
        if(monto < 9000){
            aumento = monto * 0.02;
            deposito += (monto + aumento);
        }
       
        return deposito;
    }
    
}
